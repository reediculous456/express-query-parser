{
  "name": "express-query-parser2",
  "version": "2.0.1",
  "description": "A parser helps you parse request for your express server",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "type": "commonjs",
  "author": "Wesley Reed",
  "keywords": [
    "express",
    "express5",
    "query",
    "parser",
    "query-parser",
    "middleware"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --sourcemap --clean --dts",
    "lint": "eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --config ./jest.config.js",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "prepare": "husky && npm run build",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags",
    "audit": "audit-ci --config .audit-ci.json",
    "check-updates": "ncu --configFileName .check-updates.json",
    "check-updates:fix": "ncu --configFileName .check-updates.json --upgrade --errorLevel 1",
    "check-updates:all": "ncu --configFileName .check-updates.json --target latest --errorLevel 1"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "bugs": {
    "url": "https://github.com/reediculous456/express-query-parser/issues"
  },
  "homepage": "https://github.com/reediculous456/express-query-parser#readme",
  "peerDependencies": {
    "express": "^4.17.1 || ^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@swc/core": "^1.13.20",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "audit-ci": "^7.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-sort-keys-custom-order": "^2.2.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "npm-check-updates": "^19.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2"
  }
}
